<UserControl x:Class="TaskList.Projects.Projects"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TaskList.Projects"
             mc:Ignorable="d"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             d:DataContext="{d:DesignInstance Type=local:ProjectsViewModel_Design, IsDesignTimeCreatable=True}"
             d:DesignHeight="450" d:DesignWidth="150">
    <StackPanel Background="White">
        <TextBlock Text="PROJECTS" Margin="0,0,0,50"/>
        <TextBlock Text="Do Stuff 001"/>
        <TextBlock Text="Do Stuff 002"/>
        <TextBlock Text="{Binding SomeText}"/>
        <Button Content="Click Me" Command="{Binding ButtonHasBeenClickedCommand}" />
    </StackPanel>
</UserControl>
